{
  "env": {
    "development": {
      "presets": [
        "env",
        "@babel/typescript"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
      ]
    },
    "production": {
      "presets": ["env", "minify"],
      "plugins": [
        "add-module-exports"
      ]
    }
  }
}
